#!/bin/bash

alias cs_dot='cs_create_and_open nodeselector/dotfiles main'
alias cs_ghm='cs_create_and_open github/github master'
alias cs_restart_actions='(cd /workspaces/github && script/actions/stop-actions && script/actions/start-actions)'
alias cs_restart_dotcom='(cd /workspaces/github && script/server)'
alias cs_restart_launch='(cd /workspaces/actions/launch && script/build && service launch restart)'
alias k='kubectl'
alias l='exa --icons --git --tree -F'
alias rel='dot_reload'
alias t='tmux'
alias vsg='vscode_open_nwo'